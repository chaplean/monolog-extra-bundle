{
    "name": "chaplean/monolog-extra-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Provides support for extra fields in request header",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "https://www.chaplean.coop"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chaplean\\Bundle\\MonologExtraBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Chaplean\\Bundle\\MonologExtraBundle\\": "Tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/config": "^3.0 || ^4.0",
        "symfony/console": "^3.0 || ^4.0",
        "symfony/dependency-injection": "^3.0 || ^4.0",
        "symfony/monolog-bundle": "^3.0 || ^4.0"
    },
    "require-dev": {
        "chaplean/coding-standard": "^1.1",
        "mockery/mockery": "^1.2",
        "symfony/phpunit-bridge": "^4.0",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "config": {
        "branche-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
